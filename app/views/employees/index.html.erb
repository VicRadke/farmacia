<p id="notice"><%= notice %></p>
  </div>
  <div class="col-sm">
    <h1>Empleados</h1>
  </div>
  
  </div>
  <div class="col-sm">
<table>
  <thead>
    <tr>
    <nav aria-label="breadcrumb">
    <ol class="breadcrumb">
    <ul>  
      <th>Nombre &nbsp;</th>
      <th>Apellido &nbsp;</th>
      <th>Fecha Nacimiento &nbsp;</th>
      <th>Telefono &nbsp;</th>
      <th>Sexo &nbsp;</th>
      <th colspan="3"></th>
      </ol>
      </nav>
      </tr>
  </thead>
  </div>

  <tbody>
    <% @employees.each do |employee| %>
      <tr>
        <td><%= employee.name %></td>
        <td><%= employee.last_name %></td>
        <td><%= employee.birthday %></td>
        <td><%= employee.cellphone %></td>
        <td><%= employee.sex %></td>
        <td><%= link_to 'Mostrar', employee %></td> 
        <td><%= link_to 'Editar', edit_employee_path(employee) %></td>
        <td><%= link_to 'Eliminar', employee, method: :delete, data: { confirm: 'Estas seguro?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Nuevo Empleado', new_employee_path %>
<br><br>
<%= link_to 'Inicio', '/' %>